<?xml version="1.0" encoding="UTF-8"?>
<rendertheme version="1" xmlns="http://opensciencemap.org/rendertheme">
    <m cat="nature" k="natural">
        <m select="first" zoom-min="15">
            <m v="cave_entrance">
                <symbol src="assets:symbols/natural/cave.svg" symbol-color="color:mountains" symbol-percent="70" />
            </m>
            <m v="rock">
                <symbol src="assets:symbols/natural/rock.svg" symbol-color="color:mountains" symbol-percent="50" />
            </m>
            <m v="stone" zoom-min="16"> <!-- TODO needs mapcreator -->
                <symbol src="assets:symbols/tourist/stone.svg" symbol-color="color:mountains" symbol-percent="50" />
            </m>
            <m v="spring">
                <symbol src="assets:symbols/natural/spring.svg" merge-gap="0" symbol-color="color:amenity-water" symbol-percent="70" />
            </m>
        </m>
        <m select="when-matched">
            <text caption="true" dy="12" fill="color:natural-text" size="13" stroke="color:text-outline" stroke-width="1.5" />
        </m>
    </m>
</rendertheme>